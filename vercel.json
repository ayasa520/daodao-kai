{
	"version": 2,
	"rewrites": [{
		"source": "/api/(.*)",
		"destination": "/api/index.py"
	},{
		"source": "/create",
		"destination": "/static/create.html"
	},{
		"source": "/",
		"destination": "/static/index.html"
	},{
		"source": "/login",
		"destination": "/static/login.html"
	},{
		"source": "/css/index.css",
		"destination": "/static/css/index.css"
	},{
		"source": "/css/login.css",
		"destination": "/static/css/login.css"
	},{
		"source": "/css/create.css",
		"destination": "/static/css/create.css"
	},{
		"source": "/js/login.js",
		"destination": "/static/js/login.js"
	}]
}